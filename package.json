{
  "name": "@monocloud/base",
  "version": "0.0.1",
  "description": "MonoCloud Javascript Base Library",
  "keywords": [
    "monocloud",
    "base",
    "sdk",
    "node"
  ],
  "homepage": "https://www.monocloud.com",
  "bugs": {
    "url": "https://github.com/monocld/monocloud-base-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monocld/monocloud-base-js.git"
  },
  "license": "MIT",
  "author": {
    "name": "MonoCloud",
    "email": "dev@monocloud.com"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
